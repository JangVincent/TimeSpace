{
    "name": "timespace",
    "displayName": "TimeSpace",
    "version": "0.1.0",
    "publisher": "Phantola",
    "description": "Show UTC/Locale time",
    "license": "MIT",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Phantola/TimeSpace.git"
    },
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "TimeSpace",
            "type": "object",
            "properties": {
                "timeSpace.formatUTC": {
                    "type": "string",
                    "default": "YYYY-MM-DD HH:mm:ss",
                    "description": "UTC time string format"
                },
                "timeSpace.formatLocale": {
                    "type": "string",
                    "default": "YYYY-MM-DD HH:mm:ss",
                    "description": "UTC time string format"
                },
                "timeSpace.copyFormatUTC": {
                    "type": "string",
                    "default": "YYYY-MM-DD HH:mm:ss",
                    "description": "UTC time string format"
                },
                "timeSpace.copyFormatLocale": {
                    "type": "string",
                    "default": "YYYY-MM-DD HH:mm:ss",
                    "description": "UTC time string format"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "dayjs": "^1.11.6"
    }
}
